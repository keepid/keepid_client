{
    "root": "dist/",
    "routes": {
      "/**": "index.html"
    },
    "https_only": true,
    "clean_urls": true,
    "headers": {
        "/**": {
            "Content-Security-Policy": "frame-ancestors 'self'",
            "X-Frame-Options": "SAMEORIGIN",
            "X-XSS-Protection": "1; mode=block"
        }
      }
  }