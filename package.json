    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "autoprefixer": "6.7.2",
    "babel-preset-react-app": "^2.2.0",
    "babel-runtime": "^6.20.0",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "4.1.0",
    "connect-history-api-fallback": "1.3.0",
    "copy-webpack-plugin": "^6.0.1",
    "cross-env": "^7.0.2",
    "cross-spawn": "4.0.2",
    "css-loader": "^3.5.3",
    "detect-port": "1.1.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.5",
    "enzyme-to-json": "3.6.1",
    "eslint": "^7.28.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-react-app": "^0.6.2",
    "eslint-loader": "1.6.0",
    "eslint-plugin-flowtype": "2.21.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "extract-text-webpack-plugin": "1.0.1",
    "fs-extra": "0.30.0",
    "html-webpack-plugin": "2.24.0",
    "http-proxy-middleware": "^0.20.0",
    "husky": "^2.7.0",
    "jest-fetch-mock": "3.0.3",
    "json-loader": "0.5.4",
    "lint-staged": "^9.5.0",
    "msw": "^0.26.2",
    "node-hot-loader": "^1.20.0",
    "object-assign": "4.1.1",
    "postcss-loader": "1.2.2",
    "prettier-eslint": "^12.0.0",
    "promise": "7.1.1",
    "react-cosmos": "^5.5.3",
    "react-dev-utils": "^11.0.4",
    "react-hot-loader": "^4.12.21",
    "redirect": "^0.2.0",
    "style-loader": "0.13.1",
    "stylelint": "^12.0.1",
    "ts-jest": "26.4.4",
    "url-loader": "^2.2.0",
    "webpack": "^4.44.2",
    "whatwg-fetch": "2.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "cross-env INLINE_RUNTIME_CHUNK=false react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint:scss": "stylelint 'src/**/*.scss' --syntax scss",
    "lint:scss:fix": "stylelint 'src/**/*.scss' --syntax scss --fix",
    "lint:ts": "eslint . --ext .ts,.tsx",
    "lint:ts:fix": "npm run lint:ts -- --fix",
    "cosmos": "FAST_REFRESH=false cosmos",
    "cosmos:export": "cosmos-export"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/index.tsx",
      "!src/serviceWorker.ts",
      "!src/reportWebVitals.ts"
    ],
    "coveragePathIgnorePatterns": [
      "./src/*/*.types.{ts,tsx}",
      "./src/index.tsx",
      "./src/serviceWorker.ts"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text-summary",
      "clover"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 95,
        "branches": 95,
        "lines": 95,
        "functions": 95
      }
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/ts-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "npm run --silent lint:ts:fix",
      "git add"
    ],
    "src/**/*.scss": [
      "npm run --silent lint:scss:fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "cross-env CI=true npm test"
    }
  },
  "stylelint": {
    "rules": {
      "block-no-empty": null,
      "color-no-invalid-hex": true,
      "comment-empty-line-before": [
        "always",
        {
          "ignore": [
            "stylelint-commands",
            "after-comment"
          ]
        }
      ],
      "declaration-colon-space-after": "always",
      "indentation": [
        "tab",
        {
          "except": [
            "value"
          ]
        }
      ],
      "max-empty-lines": 2,
      "rule-empty-line-before": [
        "always",
        {
          "except": [
            "first-nested"
          ],
          "ignore": [
            "after-comment"
          ]
        }
      ],
      "unit-whitelist": [
        "em",
        "rem",
        "%",
        "s",
        "vh",
        "vw",
        "deg"
      ]
    }
  }
}
